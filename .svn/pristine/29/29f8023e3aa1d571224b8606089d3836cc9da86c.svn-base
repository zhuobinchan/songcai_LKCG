<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.songcai.lkcg.domain">
 <class name="com.songcai.lkcg.domain.Administrator" table="songcai_administrator">
  <id column="管理员id" name="administrator_id" type="long">
   <generator class="identity"/>
  </id>
  <property column="管理员姓名" name="administrator_name" type="string"/>
  <property column="管理员密码" name="administrator_password" type="string"/>
 </class>
</hibernate-mapping>
